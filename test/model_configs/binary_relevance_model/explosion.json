{
    "name": "Explosion",
    "description": "Identifies Explosion vs other audio in forest areas",
    "aggregatePositiveLabelName": "Explosion",
    "aggregateNegativeLabelName": "EverythingElse",
    "positiveLabels": [
        "Fireworks",
        "Burst, pop",
        "Eruption",
        "Gunshot, gunfire",
        "Explosion",
        "Boom",
        "Fire"
    ],
    "negativeLabels": null,
    "ontology": {
        "useYoutubeAudioSet": true,
        "extension": "data/ioc_ontology_extension.json"
    },
    "networkCfgJson": "model_configs/binary_relevance_model/topology/Conv1D500-128x128_Dense500_Dense500_Dense500_Dense1_MaxPool.json",
    "train": {
        "epochs": 5,
        "batchSize": 500,
        "validationSplit": 0.3,
        "inputDataFrames": [
            {
                "path": "diff_class_datasets/Datasets/pure/Explosion/pure_exp_7957.pkl",
                "positiveSubsample": 1500,
                "negativeSubsample": 0
            },
            {
                "path": "diff_class_datasets/Datasets/pure/Domestic/pure_dom_9497.pkl",
                "positiveSubsample": 0,
                "negativeSubsample": 500
            },
            {
                "path": "diff_class_datasets/Datasets/pure/Tools/pure_tools_8113.pkl",
                "positiveSubsample": 0,
                "negativeSubsample": 500
            },
            {
                "path": "diff_class_datasets/Datasets/pure/Wild/pure_wild_7061.pkl",
                "positiveSubsample": 0,
                "negativeSubsample": 500
            }
        ],
        "outputWeightFile": "test/weights/binary_relevance_model/explosion.h5"
    }
}
